            bln10=version_code(jData)
            assert bln10
            bln11=price(jData)
            assert bln11
            bln12=download_count(jData)
            assert bln12
            bln13=purchase(jData)
            assert bln13
            
            list_limit = jResp['limit']
            app_id_list=get_app_id_list(jData)
            bln5=cat_listId_len(app_id_list,list_limit)
            self.assertTrue(bln5) 
